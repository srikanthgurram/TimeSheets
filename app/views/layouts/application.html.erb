              </ul>
            </li> <!-- Admin tasks -->
          </ul><!-- Left Nav items -->
          
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container">
    	<div class="starter-template">
        <h1>
          <span class="company">Company</span>
          <span>Time Sheets</span>
         </h1> 
      </div>

    <div class="row">
        <%= render 'layouts/partials/flashmessages' %>
